pipeline{
	agent any
	stages{
		stage('Checkout'){
			steps{
				checkout scm
			}
		}
		stage('Build'){
			steps{
				echo 'Successful'
			}
		}
		stage('Test') { 
            steps {
                echo 'testing the application...'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'deploying the application...'
            }
        }
    }
}